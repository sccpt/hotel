page {
  background: #f1f1f2;
}

.banner {
  width: 100%;
  height: 380rpx;
  position: relative;
}

.swiper {
  width: 100%;
  height: 380rpx;
}

.picNum {
  width: 110rpx;
  height: 40rpx;
  border-radius: 20rpx;
  background: rgba(0, 0, 0, 0.4);
  position: absolute;
  right: 30rpx;
  bottom: 10rpx;
  z-index: 66;
  line-height: 40rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}

.info {
  background: #fff;
  padding: 20rpx;
}

.info .title {
  padding-top: 20rpx;
  font-size: 38rpx;
}

.info .content {
  width: 100%;
  overflow: hidden;
  margin-top: 20rpx;
}

.info .item {
  width: 33.3%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  font-size: 32rpx;
  float: left;
}

.list {
  padding: 20rpx;
  background: #fff;
  margin-top: 30rpx;
}

.list .title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: #f1f1f2 1px solid;
  font-size: 32rpx;
}

.list .content {
  width: 100%;
  overflow: hidden;
  padding-top: 30rpx;
  font-size: 32rpx;
  line-height: 44rpx;
}

.footer {
  width: 100%;
  height: 130rpx;
  background: #fff;
  overflow: hidden;
  position: fixed;
  padding-top: 30rpx;
  left: 0;
  bottom: 0;
  z-index: 66;
}

.footer .button {
  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 20rpx;
  color: #fff;
  font-size: 38rpx;
  background: linear-gradient(to right, #cfa561, #dbbb7e);
}